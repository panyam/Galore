    "music",
    "carnatic"
  ],
  "dependencies": {
    "@handlebars/allow-prototype-access": "^1.0.3",
    "@panyam/tsutils": "",
    "@panyam/tsutils-gae": "",
    "@panyam/tsutils-ui": "",
    "@types/express": "^4.17.11",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.26",
    "@typescript-eslint/parser": "^4.15.0",
    "express": "^4.17.1",
    "express-handlebars": "^5.2.0",
    "express-session": "^1.17.1",
    "express-validator": "^6.9.2",
    "galore": "",
    "http-errors": "~1.8.0",
    "js-yaml": "^4.0.0",
    "tlex": "",
    "uuid": "^8.3.2",
    "uuid-v4-validator": "^2.0.0",
    "uuidv4": "^6.2.6"
  },
  "bugs": {
    "url": "https://github.com/panyam/galore/issues"
  },
  "pre-commit": [
    "precommit-msg",
    "clean",
    "build",
    "lintfix",
    "test"
  ],
  "scripts": {
    "clean": "rm -Rf dist && jest --clearCache && rm -rf static/dist",
    "buildhp": "cd homepage ; hugo -D",
    "build": "tsc -p tsconfig.server.json &&  cp *.yaml ./dist/ && cp -r bin ./dist/ && cp package.json ./dist/ && cp -r src/views ./dist/src/",
    "buildfeprod": "webpack --mode=production && npm run copystatic",
    "buildfe": "webpack --mode=development && npm run copystatic",
    "copystatic": "cp -r static/* ./dist/static/",
    "buildadmin": "tsc -p ./src/admin/tsconfig.json",
    "buildall": "npm run buildhp && npm run buildadmin && npm run buildbe && npm run buildfe",
    "buildallprod": "npm run buildsites && npm run buildadmin && npm run buildbe && npm run buildfe",
    "test": "jest --coverage --runInBand",
    "start": "node ./bin/www",
    "festart": "webpack-dev-server --mode=development",
    "fedev": "webpack-dev-server --mode=development",
    "fedebug": "webpack-dev-server --config ./webpack.config.js --mode=development --debug=true",
    "deploy": "npm run buildall && ( cd dist ; gcloud app deploy --verbosity=info )",
    "lint": "eslint './src' --quiet --ext .ts --resolve-plugins-relative-to .",
    "lintfix": "eslint 'src/**/*.ts' --quiet --fix --resolve-plugins-relative-to .",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "copy-config": "cp src/config/config.yml dist/src/config",
    "copy-static": "cp -r backend/static dist/backend",
    "tsc": "./node_modules/typescript/bin/tsc; npm run copy-config && npm run copy-static",
    "admin": "node ./cli/admin/index.js"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.13",
    "@babel/preset-flow": "^7.12.13",
    "@babel/preset-typescript": "^7.12.16",
    "@types/fs-extra": "^9.0.7",
    "@types/jest": "^26.0.22",
    "@types/js-yaml": "^4.0.0",
    "@types/jsdom": "^16.2.6",
    "@types/shelljs": "^0.8.8",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "autoprefixer": "^10.2.4",
    "babel-core": "^6.26.3",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^3.0.0",
    "codecov": "^3.8.1",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.0.2",
    "cssnano": "^4.1.10",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.2.0",
    "fs-extra": "^9.1.0",
    "handlebars": "^4.7.6",
    "handlebars-loader": "^1.7.1",
    "highlight.js": "10.6.0",
    "html-webpack-plugin": "^5.0.0",
    "html-webpack-tags-plugin": "^3.0.0",
    "jest": "^26.6.3",
    "jest-each": "^26.6.2",
    "jest-mock": "^26.6.2",
    "jsdom": "^16.4.0",
    "mini-css-extract-plugin": "^1.3.6",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^5.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.2.1",
