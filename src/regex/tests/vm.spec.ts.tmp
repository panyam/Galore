
  test("Test Lines", () => {
    const prog = compile(
      null,
      new Rule("^ *a*", 0, 20),
      new Rule("b*$", 1, 15),
      new Rule(`[ \t\n\r]+`, 2, 10),
      new Rule(".", 3, 0),
    );
    testInput(
      prog,
      `
      aaaaabcdefgh
      bbbbb
      xhzy bbb ccc
    `,
      [],
      true,
      "/tmp/testlines.html",
    );
  });
